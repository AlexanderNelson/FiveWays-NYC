<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.fiveways" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BeachActivity"
            android:label="@string/title_activity_beach" />
        <activity
            android:name=".ShakespeareActivity"
            android:label="@string/title_activity_shakespeare" />
        <activity
            android:name=".SmorgasburgActivity"
            android:label="@string/title_activity_smorgasburg" />
        <activity
            android:name=".ParkActivity"
            android:label="Park" />
        <activity
            android:name=".HighLineActivity"
            android:label="@string/title_activity_high_line">
        </activity>
    </application>

</manifest>